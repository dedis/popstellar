{
  "expo": {
    "name": "PopApp",
    "slug": "PopApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/resources/assets/icon.png",
    "splash": {
      "image": "./src/resources/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "./src/resources/**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/resources/assets/favicon.png"
    },
    "entryPoint": "./src/App.tsx"
  }
}
