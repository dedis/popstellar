    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild-web": "node node_modules/expo-optimize/build/index.js",
    "build-web": "expo build:web",
    "postbuild-web": "zip-build web-build -t fe1-$(git rev-parse HEAD)$(git diff --quiet || echo '-dirty').%EXT%",
    "eject": "expo eject",
    "startServer": "node __tests__/utils/LocalMockServer.js",
    "startMonitorServer": "nodemon __tests__/utils/LocalMockServer.js",
    "test": "jest",
    "eslint": "eslint .",
    "prettier": "prettier --check .",
    "prettier-format": "prettier --write .",
    "depcruise": "depcruise --config .depcruise.js -- src",
    "depcruise-graph": "depcruise --config .depcruise.js --output-type dot -- src | dot -T svg > dist/graph.svg",
    "depcruise-graph-archi": "depcruise --config .depcruise.js --output-type cdot -- src | dot -T svg > dist/archi.svg",
    "depcruise-graph-focus": "depcruise --config .depcruise.js --focus %npm_config_pkgfocus% --output-type dot -- src | dot -T svg > dist/focus.svg",
