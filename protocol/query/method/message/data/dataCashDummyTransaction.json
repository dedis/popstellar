{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/dedis/popstellar/master/protocol/query/method/message/data/dataCashDummyTransaction.json",
    "title": "Post a digital cash transaction",
    "type": "object",
    "properties": {
      "object": {
        "const": "transaction"
      },
      "action": {
        "const": "add"
      },
      "Amount": {
        "type": "integer",
        "description": "Amount"
        },
    "addresse_sender": {
        "type": "string",
        "contentEncoding": "base64",
        "description": "Address Sender"
        },
    "addresse_receiver": {
            "type": "string",
            "contentEncoding": "base64",
            "description": "Address Receiver"
        }
    },
    "required": ["amount", "addresse_sender", "addresse_receiver"]
  }