{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/dedis/popstellar/master/protocol/main_public_key.json",
    "title": "QRCode data for the user's main public key",
    "description": "Match a data contained in a qrcode encoding the main public key",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "main_public_key": {
            "type": "string",
            "contentEncoding": "base64",
            "description": "The main public key"
        }
    },
    "required": ["main_public_key"]
}
