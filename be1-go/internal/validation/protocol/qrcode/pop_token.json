{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/dedis/popstellar/master/protocol/qrcode/pop_token.json",
    "title": "QRCode data for pop tokens",
    "description": "Match a data contained in a qrcode encoding a pop token",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "pop_token": {
            "type": "string",
            "contentEncoding": "base64",
            "description": "The pop token"
        }
    },
    "required": ["pop_token"]
}
