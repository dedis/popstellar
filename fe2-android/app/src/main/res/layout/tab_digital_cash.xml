<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/tab_digital_cash_only"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline_horizontal_tab_digital_cash"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="@dimen/guideline_horizontal_tab" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline_vertical_tab_digital_cash"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.89" />

  <Button
    android:id="@+id/tab_digital_cash"
    android:layout_width="36dp"
    android:layout_height="43dp"
    android:text="@string/transaction"
    android:textSize="@dimen/size_small"
    app:layout_constraintBottom_toTopOf="@+id/guideline_horizontal_tab_digital_cash"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="@+id/guideline_vertical_tab_digital_cash"
    app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
